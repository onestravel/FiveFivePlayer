<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        >
        <shape>
            <corners android:radius="50dp" />
            <solid android:color="@color/five_color_transparent" />
            <stroke android:width="2dp" android:color="@color/five_color_progress_bg_color"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="50dp" />
                <solid android:color="@color/five_color_transparent" />
                <stroke android:width="2dp" android:color="@color/five_color_secondary_progress_color"/>
            </shape>
        </clip>
    </item>

    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="50dp" />
                <solid android:color="@color/five_color_transparent" />
                <stroke android:width="2dp" android:color="@color/five_color_progress_color"/>
            </shape>
        </clip>
    </item>
</layer-list>
